{
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "overrides": {
    "files": ["test/**/*-test.js"],
    "env": {
      "mocha": true
    }
  },
  "root": true,
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error"
  }
}
